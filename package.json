{
  "name": "neo4jmapper",
  "description": "Neo4jMapper is a client- and serverside object mapper for neo4j graphdatabases",
  "version": "0.8.9",
  "author": "Philipp Staender <philipp.staender@gmail.com>",
  "homepage": "https://github.com/pstaender/neo4jmapper/",
  "main": "./src/index",
  "repository": {
    "type": "git",
    "url": "git://github.com:pstaender/neo4jmapper"
  },
  "keywords": [
    "neo4j",
    "graphdatabase",
    "orm",
    "graph",
    "database",
    "driver",
    "rest",
    "client"
  ],
  "scripts": {
    "test": "coffee -mcb test/ && mocha; npm run clean",
    "clean": "find test -name '*.js' -delete -o -name '*.map' -delete",
    "preinstall": "npm i -g coffee-script mocha-lcov-reporter",
    "blanket": {
      "pattern": "src"
    }
  },
  "devDependencies": {
    "mocha": "~1.9",
    "expect.js": "~0.2",
    "should": "~1.2",
    "source-map-support": "*",
    "futures": "~2.3.1",
    "join": "~2.3",
    "coveralls": "~2.0.16",
    "mocha-lcov-reporter": "0.0.1",
    "blanket": "~1.1.5",
    "jscoverage": "~0.3.7",
    "coffee-script": "~1.6.3"
  },
  "dependencies": {
    "underscore": "~1.4",
    "jquery": "~1.8",
    "superagent": "~0.15.1",
    "JSONStream": "~0.6.4"
  },
  "licenses": {
    "type": "GPL"
  }
}
